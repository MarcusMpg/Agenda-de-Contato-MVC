@model ContatoModel;
@{
    ViewData["Title"] = "Apagar contato";
}

<div class="text-center">
    <h3>Deseja apagar o contato(@Model.Nome) ?</h3>

</div>
<div class="text-end">
    <a class="btn btn-primary" role="button" asp-controller="Contato" asp-action="Index">Concelar</a>
    <a class="btn btn-primary" role="button" asp-controller="Contato" asp-action="Excluir" asp-route-id="@Model.Id">Confirmar</a>
</div>

